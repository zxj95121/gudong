}
.index-ct{
	width:100%;
	margin-top:0.5rem;
}
.index-ct-nav{
	width: 100%;
	padding: 0 4px;
	box-sizing: border-box;
	background: #fff;
	height: 3.2rem;
	line-height: 3rem;
	overflow-x: scroll;
	overflow-y: hidden;
	box-shadow:1px 0px 4px #ccc;
}
.index-ct-nav li{
	float: left;
	width: 4.75rem;
	text-align: center;
}
.index-active{
	border-bottom:2px solid #df3361;
}
.index-ct-list{
	width:100%;
	padding:0 3%;
	box-sizing: border-box;
}
.index-ct-list li{
	margin-top:0.8rem;
	height:10rem;
	box-shadow:1px 0px 4px #ccc;
}
.index-ct-list-item1{
	background:url(../images/img-ka01.png) no-repeat;
	width:100%;
	height:100%;
	background-size:100%!important;
	border-radius:3px;
}
.index-ct-list-item2{
	background:url(../images/img-ka02.png) no-repeat;
}
.index-ct-list-item1-left{
	float:left;
	margin-top:4.5rem;
	margin-left:1rem;
    width:18rem;
}
.index-ct-list-item1-left h1{
	color:#fff;
	font-size:0.8rem;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    height: 2rem;
    margin-bottom: 1.1rem;
}
.index-ct-list-item1-left h2{
	color:#fff;
	font-size:1.2rem;
	margin-top:0.4rem;
}
.index-ct-list-item1-left h3{
	color:#fff;
	font-size:0.8rem;
	margin-top:0.4rem;
}
.index-ct-list-item1-right,.index-ct-list-item1-right1{
	float:right;
	margin-right:1rem;
	margin-top:5.5rem;
}
.index-ct-list-item1-right button,.index-ct-list-item1-right1 button{
	border:none;
	width:3.5rem;
	height:3.5rem;
	border-radius:50%;
	color:#fff;
}
.index-ct-list-item1-right button{
	background:#e32490;
}
.index-ct-list-item1-right1 button{
	background:#666;
}

.store-detail{
	width:100%;
}
.store-detail img{
	width:100%;
	height:100%;
}
.store-detail-info1,.store-detail-info2,.store-detail-info3{
	width:100%;
	height:3rem;
	line-height:3rem;
	padding:0 3%;
