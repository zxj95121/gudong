@charset "utf-8";
/* CSS Document */
html{
	font-size:62.5%; /* 10รท16=62.5% */
}
* {
	margin: 0;
	padding: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-o-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-tap-highlight-color: transparent;
	font-family: "PingFang SC", PingFangSC, Helvetica, sans-serif!important;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    -webkit-tap-highlight-color:transparent;
    outline:none;
    -webkit-appearance: none;

}
@font-face {font-family:"myFont fan";src: url('./NotoSansHans-DemiLight.ttf');
}
body, div, span, i, em, h2, h3, h4, h5, h6, h1, b, big, small, ul, li, a, img {
  margin: 0;
  padding: 0;
  border:none;
  /*font-weight:lighter;*/
  font-size:1rem;
  font-weight:400;
  color:#323232; 
  
	-webkit-tap-highlight-color: transparent;
	font-family: "PingFang SC", PingFangSC, Helvetica, sans-serif!important;
}
input,button,textarea,text{
	outline: none;
	font-family: "PingFang SC", PingFangSC, Helvetica, sans-serif!important;
}
li{
  list-style:none;
  }
a{
  text-decoration:none;
  }
.clear-fix:after{
	content:' ';
	width:0;
	height:0;
	clear:both;
	display:block;
}
em,i{
	font-style:normal;
}

.top{
	width:100%;
	font-size:1.2rem;
	padding:0.8rem 0;
	color:#fff;
	text-align:center;
	/*position:relative;*/
	background:#323232;
	box-shadow:1px 0px 4px #ccc;
}
.top-left{
	left:3%;
	position:absolute;
	top:1rem;
}
.top-left img{
	height:1rem;
}
.user-head,.user-nav,.user-info-head,.user-info,.user-wallet-list{
	background:#fff;
	width:100%;
	margin-top:0.5rem;
	box-shadow:1px 0px 4px #ccc;
}
.user-head{
	padding:2rem 10% ;
	box-sizing: border-box;
	border-bottom:1px solid #eee;
}
.user-head-left{
	float:left;
}
.user-head-right{
	float:right;
}
.user-head-right img{
	width:4.5rem;
	height:4.5rem;
}
.user-head-left h1{
	float:left;
	margin-top:1.1rem;
}
.user-head-left img{
	float:left;
	width:0.5rem;
	margin-left:1rem;
	margin-top:1.35rem;
}
.user-head-left span{
	display:block;
	margin-top:3rem;
	color:#b7b7b7;
	font-size:0.9rem;
}
.user-nav li{
	padding:0 10% ;
	box-sizing: border-box;
	border-bottom:1px solid #eee;
	height:4rem;
	line-height:4rem;
}
.user-nav li img{
	float:left;
	height:1.2rem;
	margin-right:1.6rem;
	margin-top:1.4rem;
    width:1.3rem;
}
.user-nav li span{
	float:left;
}
.navBar{
	width:100%;
	height:3.5rem;
	background:#fff;
	position:fixed;
	bottom:0;
	box-shadow:1px -1px 4px #ccc;
    z-index: 9999;
}
#footer a:hover{
  text-decoration: none;
}
.navBar li{
	float:left;
	width:33.3%;
	text-align:center;
	cursor: pointer;
}
.navBar li img{
	width:1.2rem;
	height:1.2rem;
	margin-top:0.6rem;
}
.navBar li span{
	display:block;
	font-size:0.6rem;
	color:#666;
}
.user-info-head{
	padding:0 10%;
	box-sizing: border-box;
	height: 5rem;
    line-height: 5rem;
}
.user-info-head h1,.user-info li h1{
	float:left;
}
.user-info-head img{
	float:right;
	width:3rem;
	margin-top:1rem;
}
.user-info{
    padding: 0.5rem 10% 1.5rem 10%;
	box-sizing: border-box;
}
.user-info li{
	height:3rem;
	line-height:3rem;
}
.user-info li img{
	float:right;
	width:0.6rem;
	margin-top:1rem;
	margin-left:0.8rem;
}
.user-info li span{
	float:right;
	color:#999;
}
.user-info li input{
	float:right;
	text-align:right;
	margin-top:0.8rem;
	border:none;
}
.user-wallet{
	background:#fff;
	width:100%;
	padding:0.6rem 3%;
	box-sizing: border-box;
	box-shadow:1px 0px 4px #ccc;
}
.user-wallet span{
	color:#df3361;
	margin-right:0.2rem;
}
.user-wallet-list-ct,.user-wallet-list h1{
	padding:0.5rem 3%;
	box-sizing: border-box;
}
.user-wallet-list h1{
	text-align:center;
	border-bottom:1px solid #eee;
}
.user-wallet-list-ct li {
	float:left;
	width:47%;
	margin-right:0.7rem;
	margin-top:1rem;
}
.user-wallet-list-ct li a{
	display:block;
	width:100%;
	height:3.5rem;
	border:1px solid #1985ff;
	border-radius:2px;
	text-align:center;
	padding-top:0.4rem;
	padding-bottom:0.3rem;
	
}
.user-wallet-list-ct li a span{
	display: block;
	color: #1985ff;
	line-height: 1.5rem;
}
::-webkit-scrollbar
{
    width: 0;
    height: 0;
}
.user-wallet-list-ct li a em{
	display: block;
	color: #1985ff;
	font-size: 0.6rem;
	/* margin-top: 0.2rem; */
	line-height: 1.2rem;
}
.user-wallet-list-ct  li a:active,.user-wallet-list-ct  li a:visited{
	/*border:none;*/
	background:#1985ff;
	color:#fff;
}
.user-wallet-list button{
	text-align:center;
	border:none;
	background:#df3361;
	border-radius:3px;
	color:#fff;
	margin-top:2rem;
	width:30%;
	height:2.5rem;
	margin-left:35%;
	margin-bottom:1rem;
	font-size:1.1rem;
}
.user-wallet-list-f{
	text-align:center;
	font-size:0.6rem;
	padding-bottom:1rem;
}
.user-wallet-list-f em{
	font-size:0.6rem;
	color:#1985ff;	
}

.user-coupon{
	width:100%;
	padding:0 3%;
	box-sizing: border-box;
	margin-top:1rem;
}
.user-coupon-sele{
	float:left;
	margin-top: 2.97rem;
}
.user-coupon-sele img{
	width:1rem;
}
.user-coupon-ct{
	float:left;
	background:url(../images/bg-img01.png) no-repeat;
	background-size:100% 100%;
	width:92%;
	/*margin-left:3%;*/
	padding:1rem 3%;
	box-sizing: border-box;
	height:7rem;
}
.user-coupon-ct-img{
	width: 5rem;
    height: 5rem;
	float:left;
}
.user-coupon-ct-img img{
	max-width:100%;
	max-height:100%;
}
.user-coupon-ct-tl{
	float:left;
	margin-left:0.5rem;
	width:47%;
}
.user-coupon-ct-tl h2{
	font-size:0.6rem;
	color:#b7b7b7;
	margin-top:1rem;
}
.user-coupon-ct-tl h3{
	font-size:0.6rem;
	margin-top:0.2rem;;
}
.user-coupon-ct-pri{
	float:right;
	border-left:1px dotted #eee;
	text-align:center;
	padding-left:0.4rem;
	margin-top:0.6rem;
	width:23%;
}
.user-coupon-ct-pri h1{
	font-size:1.4rem;
	font-weight:bold;
	color:#df3361;
}
.user-coupon-ct-pri h2{
	font-size:0.6rem;
	color:#b7b7b7;
	margin-top:0.5rem;
}
.user-coupon-footer{
	width:100%;
	position:absolute;
	bottom:2rem;
	padding:0 5%;
	box-sizing: border-box;
}
.user-coupon-footer li{
	float:left;
	width:45%;
	height:2.5rem;
}
.user-coupon-footer-btn1{
	margin-right:5%;
}
.user-coupon-footer-btn1 button{
	border:none;
	background:#df3361;
	color:#fff;
	width:100%;
	height:100%;
	border-radius:2px;
}
.user-coupon-footer-btn2{
	margin-left:5%;
}
.user-coupon-footer-btn2 button{
	border:none;
	background:#df3361;
	color:#fff;
	width:100%;
	height:100%;
	border-radius:2px;
}
.user-comment{
	width:100%;
	background:#fff;
	padding:1rem 3%;
	box-sizing: border-box;
	border-bottom:1px solid #eee;
}
.user-comment-img{
	float:left;
}
.user-comment-img img{
	width:2.5rem;
	height:2.5rem;
}
.user-comment-right{
	float:right;
	margin-left:0.6rem;
	width:85%;
}
.user-comment-right-ct1 h1{
	font-size:0.8rem;
	float:left;
}
.user-comment-right-ct1 h2{
	font-size:0.6rem;
	color:#999;
	float:right;
}
.user-comment-right-ct2{
	margin-top:0.6rem;
}
.user-comment-right-ct2 img{
	float:left;
	width:0.8rem;
	height:0.7rem;
	margin-right:0.2rem;
}
.user-comment-right-ct3{
	font-size:0.9rem;
