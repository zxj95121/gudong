}
.user-comment-right-ct4{
	margin-top:1rem;
}
.user-comment-right-ct4 img{
	width:5rem;
	height: 3.5rem;
	margin-right:0.6rem;
}

.index-store{
	width:100%;
	background:#fff;
	padding:0.5rem 3%;
