@charset "utf-8";
/* CSS Document */

* {
	margin: 0;
	padding: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-o-box-sizing: border-box;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-tap-highlight-color: transparent;
	font-family: "PingFang SC", PingFangSC, Helvetica, sans-serif!important;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    -webkit-tap-highlight-color:transparent;
    outline:none；
}
/* @font-face {font-family:"myFont fan";src: url('./NotoSansHans-DemiLight.ttf'); */
/*}*/
body, div, span, i, em, h2, h3, h4, h5, h6, h1, b, big, small, ul, li, a, img {
  margin: 0;
  padding: 0;
  border:none;
  /*font-weight:lighter;*/
  font-size:1rem;
  font-weight:400;
  color:#323232;
	-webkit-tap-highlight-color: transparent;
	font-family: "PingFang SC", PingFangSC, Helvetica, sans-serif!important;
}
input,button,textarea,text{
	font-family: "PingFang SC", PingFangSC, Helvetica, sans-serif!important;
}
li{
  list-style:none;
  }
a{
  text-decoration:none;
  }
.clear-fix:after{
	content:' ';
	width:0;
	height:0;
	clear:both;
	display:block;
}
em,i{
	font-style:normal;
}
/****************************/
.con-top{
	background:#ffffff;
	width: 100%;
	height: 7rem;
	overflow: hidden;
}
.con-top .header{
	float:left;
	width: 5rem;
    height: 5rem;
    margin-top: 1rem;
    margin-left: 1rem;
}
.info{
		float:left;
	}
.info .name{
		font-size: 1.6rem;
    color: #464646;
    margin-top: 1.2rem;
    margin-left: 1rem;	
}
.info .detail{
	font-size: 1.1rem;
    color: #9d9d9d;
    margin-top: 0.5rem;
    margin-left: 1rem;
}
.info .king{
	float: left;
    width: 8rem;
    border-radius: 2rem;
    background: #ff6e00;
    margin-top: -1.8rem;
    margin-left: 8rem;
    padding: 0.2rem 0.5rem;
    position:relative;
   
}
.info .king img{
	width: 2rem;
    height: 1.5rem;
    position: absolute;
    top: 0.2rem;
    left: 0.5rem;
}
.info .king span{
	 color:white;
	 margin-left: 2.2rem;
}
.box{
	background: #ffffff;
    margin-top: 0.3rem;
    overflow: hidden;
    padding: 0.8rem;
}
.box .ate{
	    font-size: 1.4rem;
    color: #787878
}
.wrap {
	border: 1px solid #ff9218;
    border-radius: 3rem;
    overflow: hidden;
    width: 92%;
    margin: 1rem;
    padding: 0.6rem 1.6rem;
}
.wrap span{
	float: left;
    color: #787878
}
.wrap span.one{
	font-size: 1.2rem;
    margin-top: 0.8rem;
    margin-left: 0.8rem;
}
.wrap span.two{
	font-size: 1rem;
    margin-top: 2.6rem;
    margin-left: -6.4rem
}
.wrap span.three{
	float: right;
    font-size: 3.5rem;
    color: #ff9218
}
.pay{
	width: 90%;
    position: relative;
    padding: 1.2rem 0;
}
.pay img{
	width: 1.8rem;
    height: 1.8rem;
    position: absolute;
    top: 1.3rem;
    left: 2.5rem;
}
.pay span{
	font-size: 1.4rem;
    color: #464646;
    margin-left: 6rem;
}
.pay input{
	width: 2rem;
    height: 2rem;
    border-radius: 1rem;
    float: right
}
.list{
	background: #ffffff;
    margin-top: 0.3rem;
    overflow: hidden;
    padding: 0.8rem;
}
.list li{
	width: 100%;
    padding: 0.8rem 1.5rem;
    position: relative;
    overflow: hidden;
}
.list li input{
	width: 2rem;
    height: 2rem;
    position: absolute;
}
.list li span{
	    font-size: 1.4rem;
    color: #787878;
    margin-left: 3.6rem;
}
.foot{
	background: #fff;
    margin-top: 1rem;
    overflow: hidden;
    padding: 0.8rem 1rem;
    font-size: 1.4rem;
    padding: 0.8rem 1.4rem;
}
.foot p{
	float: left;
    font-size: 1.6rem;
    margin-top: 0.4rem;
}
.foot span{
	float: right;
    background: #961f23;
    width: 7rem;
    font-size: 1.4rem;
    color: #fff;
    padding: 0.6rem;
    cursor:pointer;
}