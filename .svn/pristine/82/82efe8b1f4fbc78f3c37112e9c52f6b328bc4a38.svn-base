(function($, doc) {
	$.init();
	var dateObj = new Array();
	var date = new Date();
	var min_year = date.getFullYear() - 70;
	for(var i=1;i<=60;i++){
	    dateObj[i-1] = new Object();
	    dateObj[i-1]['value'] = i+min_year,
	    dateObj[i-1]['text'] = i+min_year,
	    dateObj[i-1]['children'] = new Array();

