	    		} else {
	    			var kk = k;
	    		}
	    		dateObj[i-1]['children'][j-1]['children'][k-1] = new Array();
	    		dateObj[i-1]['children'][j-1]['children'][k-1]['value'] = kk;
	    		dateObj[i-1]['children'][j-1]['children'][k-1]['text'] = kk;
	    	}
	    }
	}

